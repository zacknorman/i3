[Hunter]
label=☯
command=echo"Norman Networking <> Zack Norman☯"
interval=100
color=#ffffff

separator_block_width=14

[Weather]
command=~/IconicWeather.sh "CAXX0295"
interval=1800
color=#e97ac8


[mail]
label= 
command=~/.mail.sh.x
interval=100
color=#867868

[disk-root]
label=:
command=/usr/share/i3blocks/disk /
interval=30
color=#1793D1


[disk-home]
label=:
command=/usr/share/i3blocks/disk $HOME
interval=30
color=#859900

[ssid]
label=
command=echo "$(iw dev wlp3s0 link | grep SSID | cut -d " " -f 2-)"
color=#d33682
interval=5



[wireless]
instance=wlo1
command=/usr/share/i3blocks/iface
color=#00FF00
interval=10

[strength]
command=echo "$(iw dev wlo1 link | grep signal | cut -d " " -f 2-)"
interval=10
color=#cb4b16

[temp]
label=♛
command=echo "Huntsman Online"
interval=10
color=#b58900

[battery]

command=/usr/share/i3blocks/battery

interval=30

[load]
label= 
command=/usr/share/i3blocks/load_average
interval=10
color=#6c71c4


[volume]
label=
command=echo "Pinger Online"
interval=2
signal=10
color=#75843B


[time]
label=
command=date '+%a %m-%d-%y %l:%M:%S %p'
interval=1
color=#CSD988
